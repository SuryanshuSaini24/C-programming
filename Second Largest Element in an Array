#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <limits.h>

int main() {
    int n;
    scanf("%d",&n);
    int arr[n];
    int i;
    for(i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
   int max=INT_MIN;
   int smax=INT_MIN;
    for(i=0;i<n;i++){
        if(max<arr[i]){
            max=arr[i];
            
        }
    }
    for(i=0;i<n;i++){
        if(arr[i]!=max && smax<arr[i]){
            smax=arr[i];
            
        }
        
    }
    if(smax==INT_MIN){
    
        smax=max;
        printf("No second largest element");}
    else{ 
    
    
     printf("%d",smax);
    }
    
    

    return 0;
}
