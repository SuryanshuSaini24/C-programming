#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

int A,B,C;
    scanf("%d %d %d",&A,&B,&C);
    if(A>=B&&A>=C){
    printf("%d\n",A);
    }
    else if (B>=A&&B>=C) {
        printf("%d\n",B);
    }
    else if(C>=A&&C>=B){
        printf("%d",C);
    }
    return 0;
}
