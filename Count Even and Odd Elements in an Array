#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n;
    scanf("%d",&n);
  int  arr[n];
    int i;
    for(i=0;i<n;i++){
    scanf("%d",&arr[i]);
    }
    int odd=0,even=0;
    for(i=0;i<n;i++){
        if(arr[i]%2==0){
            even++;
        }
        else{
            odd++;
            
            
        }
    }
    printf("%d %d",even,odd);


    return 0;
}
